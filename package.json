{
  "name": "nugget-bot",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "npx tsc",
    "build:changelog": "npx @discordx/changelog --root=src",
    "dev": "ts-node-esm --experimental-specifier-resolution=node src/main.ts",
    "start": "node --experimental-specifier-resolution=node build/main.js",
    "watch": "nodemon --experimental-specifier-resolution=node --exec ts-node-esm src/main.ts",
    "lint": "eslint ./src/**/*",
    "lint-fix": "eslint ./src --fix"
  },
  "dependencies": {
    "@discordx/importer": "^1.1.10",
    "@discordx/pagination": "^3.0.0",
    "discord.js": "^14.2.0",
    "discordx": "^11.1.8",
    "dotenv": "^16.0.1",
    "google-spreadsheet": "^3.3.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/google-spreadsheet": "^3.3.0",
    "@types/node": "^18.7.8",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
